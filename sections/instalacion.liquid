
<div class="contenedor-instalacion-movil">

    <div class="txt-instalacion">
        
    </div>
      
</div>

<div class="contenedor-instalacion-desktop">

</div>
<style>
    .contenedor-instalacion-movil{
        background-image: url('https://cdn.shopify.com/s/files/1/0766/8829/4173/files/Servicio_Instalacion_movil-C.png?v=1686890100');
        width: 100%;
        height: 400px;
        background-repeat: no-repeat;
        background-size: contain;
        position: relative;
    }

    @media(min-width:410px){
        .contenedor-instalacion-movil{
            background-size: cover;
            height: 600px;
        }
    }

    @media(min-width:991px){
         .contenedor-instalacion-movil{
            display: none;
         }
    }

    .contenedor-instalacion-desktop{
     display: none;
    }

    @media(min-width:991px){
        
        .contenedor-instalacion-desktop{
        background-image: url('https://cdn.shopify.com/s/files/1/0766/8829/4173/files/Instalacion_Compu_S_T-Comp.png?v=1686890212');
        width: 100%;
        height: 650px;
        background-repeat: no-repeat;
        background-size: contain;
        display: block;
    }
    }
</style>


